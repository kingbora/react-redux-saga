{
  "name": "react-music",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./bin/dev-server.js",
    "start-prod": "cross-env NODE_ENV=production node ./bin/dev-server.js",
    "build-prod-libs": "rimraf dll && cross-env NODE_ENV=production webpack --config ./build/webpack.config.dll.js --progress",
    "build-dev-libs": "rimraf dll && cross-env NODE_ENV=development webpack --config ./build/webpack.config.dll.js --progress",
    "build-prod": "rimraf www && cross-env NODE_ENV=production webpack --progress --colors -p --config ./build/webpack.config.prod.js",
    "build-dev": "rimraf www && cross-env NODE_ENV=development webpack --progress --colors -p --config ./build/webpack.config.dev.js"
  },
  "dependencies": {
    "antd": "^3.5.4",
    "axios": "^0.18.0",
    "codemirror": "^5.38.0",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "marked": "^0.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bundle-loader": "^0.5.6",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.2"
  }
}
